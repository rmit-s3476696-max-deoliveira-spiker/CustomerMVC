@model IEnumerable<CustomerMVC.Models.Cineplex>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Cineplex";
}
<h2>Cineplex</h2>
<table class="table">
    <tr>
        <th>
            Location
        </th>
        <th>
            Maps
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Location);
            </td>
            <td>
                <img src="@Html.DisplayFor(modelItem => item.ImageUrl)" alt="1" style="width:200px;height:200px;">
            </td>
        </tr>
    }

</table>

